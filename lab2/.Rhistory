annotate("rect", xmin=70, xmax=79.5, ymin=75, ymax=105, alpha=0.2)+
xlab("Time (ZT)")+ylab("Angle")+labs(title="Solar Tracking for elf3 Individuals")+theme(plot.title = element_text(hjust = 0.5))
